<%= form_for @job, url: {action: 'create'} do |f| %>
	<%= f.label :title %>
	<%= f.text_field :title %>
	<%= f.label :description %>
	<%= f.text_field :description %>
	<%= f.label :company %>
	<%= f.text_field :company %>
	<%= f.label :submission_date %>
	<%= f.date_field :submission_date %>
	<%= f.submit "Create new app" %>
<% end %>