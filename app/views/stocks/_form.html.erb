<%= form_for @form, url: lookup_path, method: "get", id: "search_form", remote: true do |f|%>
	<div>
	<%= f.label :symbol %>
	<%= f.search_field :symbol, id: "symbol", placeholder: "Search stocks" %>
	</div>

	<div>
	<%= f.label :start_date %>
	<%= f.date_field :start_date, id: "start-date", value: Date::today.prev_year %>
	</div>

	<div>
	<%= f.label :end_date %>
	<%= f.date_field :end_date, id: "end-date", value: Date::today %>
	</div>

	<div>
	<%= f.submit "Search", name: nil %>
	</div>
<% end %>